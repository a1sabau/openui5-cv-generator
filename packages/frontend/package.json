{
  "name": "ui5-cv-generator-frontend",
  "version": "1.0.0",
  "author": "Andrei Sabau",
  "license": "Apache-2.0",
  "scripts": {
    "build:start": "npm run build:opt && npm run build:clean && npm run start:bundle",
    "build:clean": "ts-node --project tsconfig-utils.json utils/dist-cleanup.ts",
    "clean": "rimraf dist coverage",
    "build": "ui5 build --clean-dest",
    "build:opt": "ui5 build self-contained --clean-dest --all",
    "build:mock": "ui5 build self-contained --clean-dest --all",
    "build:bundle": "ui5 build self-contained --include-all-dependencies --exclude-task=createDebugFiles --clean-dest",
    "build:bundle2": "ui5 build self-contained --include-all-dependencies --exclude-task=createDebugFiles --clean-dest --all",
    "build:bundle3": "ui5 build --exclude-task=* --include-task=minify generateComponentPreload --clean-dest --all",
    "build:bundle4": "ui5 build  self-contained --clean-dest",
    "start": "ui5 serve --port 8080 -o index.html --config ui5.yaml",
    "start-cdn": "ui5 serve --port 8080 -o index-cdn.html",
    "start:bundle": "npm start -- --config ui5-dist-bundle.yaml",
    "start:dist-cdn": "npm run start-cdn -- --config ui5-dist.yaml",
    "ts-typecheck": "tsc --noEmit",
    "lint": "eslint webapp",
    "test:unit": "karma start karma-ci.conf.js --ui5.testpage webapp/test/unit/unitTests.qunit.html",
    "test:unit:cov": "karma start karma-ci-cov.conf.js --ui5.testpage webapp/test/unit/unitTests.qunit.html",
    "test:integration": "karma start karma-ci.conf.js --ui5.testpage webapp/test/integration/opaTests.qunit.html",
    "test:integration:cov": "karma start karma-ci-cov.conf.js --ui5.testpage webapp/test/integration/opaTests.qunit.html",
    "test:all:cov": "karma start karma-ci-cov.conf.js"
  },
  "devDependencies": {
    "@openui5/types": "1.119.0",
    "@types/node": "^20.8.4",
    "@ui5/cli": "^3.3.1",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-ui5": "^3.0.3",
    "karma-ui5-transpile": "^0.3.22",
    "rimraf": "^5.0.1",
    "ui5-middleware-livereload": "^0.8.4",
    "ui5-middleware-simpleproxy": "^3.2.4",
    "ui5-tooling-transpile": "^0.7.17"
  }
}
